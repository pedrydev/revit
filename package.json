{
  "name": "revit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "eslint": "eslint ./src --ext .ts,.tsx --quiet --fix",
    "format": "prettier  --loglevel warn --write \"./**/*.{ts,tsx,css,json}\" "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{ts,tsx}": "eslint",
    "src/*.css": "stylelint",
    "src/**/*.ts,tsx,json,css,md}": "prettier -w -u"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@tanstack/react-query": "^4.20.9",
    "clsx": "^1.2.1",
    "i18next": "^22.4.8",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "oidc-client-ts": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^12.1.1",
    "react-oidc-context": "^2.1.1",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.20.9",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.5.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.7.2",
    "postcss-reporter": "^7.0.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^27.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^4.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.21.1"
  }
